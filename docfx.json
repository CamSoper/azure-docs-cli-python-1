{
  "build": {
    "content": [
      {
        "src": "docs-ref-autogen",
        "files": [
          "**/*.pycliyml",
          "**/*.md"
        ],
        "exclude": [
          "refTOC.md",
          "**/obj/**",
          "cli/**",
          "**/includes/**"
        ]
      },
      {
        "src": "docs-ref-conceptual",
        "files": [
          "**.md"
        ],
        "exclude":[
          "toc.md",
          "**/toc.md"
        ]
      },
      {
        "src": "breadcrumb",
        "dest": "breadcrumb",
        "files": [
          "**/*.yml"
        ]
      }

    ],
    "resource": [
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/cli/azure/breadcrumb/toc.json",
      "keywords": "azure, cli, command line",
      "brand": "azure",
      "author": "clark-allen",
      "ms.author": "allclark",
      "manager": "douge",
      "ms.topic": "reference",
      "ms.devlang": "python",
      "ms.date": "12/06/2016"
    },
    "fileMetadata": {
       "ms.service":
        {
          "cli/azure/acr/*.pycliyml": "container-registry",
          "acs/*.pycliyml": "container-service",
          "docs-ref-autogen/ad/**": "active-directory",
          "docs-ref-autogen/appservice/*.pycliyml": "app-service",
          "docs-ref-autogen/container/**": "container-service",
          "docs-ref-autogen/group/**": "azure-resource-manager",
          "docs-ref-autogen/iot/**": "iot-hub",
          "docs-ref-autogen/keyvault/**":"key-vault",
          "docs-ref-autogen/network/**":"virtual-network",
          "docs-ref-autogen/policy/**": "",
          "docs-ref-autogen/redis/**":"redis-cache",
          "docs-ref-autogen/resource/**":"azure-resource-manager",
          "docs-ref-autogen/role/**": "",
          "docs-ref-autogen/sql/**": "sql-server",
          "docs-ref-autogen/storage/**": "storage",
          "docs-ref-autogen/vm/**": "virtual-machines",
          "docs-ref-autogen/vmss/**": "virtual-machine-scale-sets"    
         },
        "hideEdit":
        {
          "**/**/*.pycliyml": "true"
        }
    },
    "template": [],
    "dest": "cli/azure"
  }
}
